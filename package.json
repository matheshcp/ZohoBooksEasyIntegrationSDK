{
  "name": "@zohobooks/sdk",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript/JavaScript SDK for integrating Zoho Books API with React applications and Node.js projects. Features complete CRUD operations for contacts, invoices, payments, and sales receipts with built-in React hooks.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "zoho",
    "books",
    "api",
    "sdk",
    "react",
    "typescript",
    "accounting",
    "invoicing",
    "contacts",
    "payments",
    "sales-receipts",
    "oauth",
    "integration",
    "business",
    "finance",
    "crm",
    "billing"
  ],
  "author": {
    "name": "Mathesh S",
    "email": "mathesh@vimix.app",
    "url": "https://github.com/matheshcp"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/zohobooks-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/zohobooks-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/zohobooks-sdk#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.6.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}